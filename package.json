{
  "name": "aureooms-js-pairing-heap",
  "description": "Pairing heap data structure library for JavaScript",
  "version": "0.0.0",
  "author": "aureooms",
  "ava": {
    "require": [
      "babel-polyfill",
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-pairing-heap/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "codeclimate-test-reporter": "^0.4.0",
    "coveralls": "^2.11.14",
    "esdoc": "^0.5.2",
    "nyc": "^10.0.0"
  },
  "homepage": "https://aureooms.github.io/js-pairing-heap",
  "keywords": ["data", "heap", "pairing", "structure"],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-pairing-heap"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "prepublish": "npm run build",
    "test": "ava ./test/src"
  }
}
