{
  "name": "@aureooms/js-pairing-heap",
  "description": "Pairing heap data structure library for JavaScript",
  "version": "0.1.1",
  "author": "aureooms",
  "ava": {
    "require": [
      "babel-polyfill",
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "env"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-pairing-heap/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@aureooms/js-heap-spec": "10.0.1",
    "ava": "0.25.0",
    "babel-cli": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "codeclimate-test-reporter": "0.5.1",
    "coveralls": "3.0.9",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "nyc": "11.9.0"
  },
  "homepage": "https://aureooms.github.io/js-pairing-heap",
  "keywords": [
    "data",
    "heap",
    "pairing",
    "structure"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-pairing-heap"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "esdoc": "esdoc",
    "prepublish": "npm run build",
    "test": "ava ./test/src"
  }
}
