{
  "name": "@aureooms/js-pairing-heap",
  "description": "Pairing heap data structure library for JavaScript",
  "version": "1.0.0",
  "license": "AGPL-3.0",
  "author": "aureooms",
  "homepage": "https://aureooms.github.io/js-pairing-heap",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-pairing-heap"
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-pairing-heap/issues"
  },
  "keywords": [
    "data",
    "heap",
    "pairing",
    "structure"
  ],
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "c8 --all --src src --reporter=lcov npm test",
    "esdoc": "esdoc",
    "lint-config": "fixpack --dryRun",
    "lint-config-and-fix": "fixpack || fixpack",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava"
  },
  "dependencies": {},
  "devDependencies": {
    "@aureooms/js-heap-spec": "15.0.3",
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.13.10",
    "@babel/register": "7.13.8",
    "ava": "3.15.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.0.1",
    "c8": "7.6.0",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "fixpack": "4.0.0",
    "np": "6.5.0",
    "power-assert": "1.6.1"
  },
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  }
}
